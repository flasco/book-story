    "react-router-cache-route": "^1.12.5",
    "react-router-dom": "^5.3.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "tslib": "^2.3.1"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": "eslint",
    "**/*.scss": "stylelint"
