{
  "name": "@leavo/book-story",
  "version": "0.0.26",
  "author": "flasco",
  "license": "MIT",
  "private": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "prepublishOnly": "yarn build",
    "npm-publish": "yarn version --patch && npm publish --registry=https://registry.npmjs.org --access public",
    "upgrade-dep": "yarn upgrade-interactive",
    "scss-lint": "stylelint src/**/*.scss",
    "es-lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@milahu/patch-package-with-pnpm-support": "^6.4.10",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "ora": "^6.1.0",
    "penv.macro": "^0.4.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.1",
    "stylelint": "^13.13.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.7.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.9.9",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-compression": "^0.4.0",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "zx": "^4.3.0"
  },
  "dependencies": {
    "@react-spring/web": "^9.4.5",
    "antd-mobile": "^5.12.6",
    "antd-mobile-icons": "^0.2.2",
    "axios": "^0.25.0",
    "classnames": "^2.3.1",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "rxjs": "^7.5.5",
    "tslib": "^2.4.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": "eslint",
    "**/*.scss": "stylelint"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@react-spring/web",
        "antd-mobile",
        "antd-mobile-icons",
