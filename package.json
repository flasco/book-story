{
  "name": "book-story",
  "version": "0.0.1",
  "author": "flasco",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "build:doc": "yarn build && node scripts/ph-page.mjs",
    "upgrade-dep": "yarn upgrade-interactive",
    "scss-lint": "stylelint src/**/*.scss",
    "es-lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
