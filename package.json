{
  "name": "book-story",
  "version": "0.0.1",
  "author": "flasco",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "husky install",
    "dev": "vite",
    "build": "vite build",
    "build:doc": "yarn build && node scripts/ph-page.mjs",
    "upgrade-dep": "yarn upgrade-interactive",
    "scss-lint": "stylelint src/**/*.scss",
    "es-lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@preact/preset-vite": "^2.1.5",
    "@types/fs-extra": "^9.0.13",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-window": "^1.8.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "@vitejs/plugin-legacy": "^1.6.2",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.14.3",
    "fs-extra": "^10.0.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.3",
    "ora": "^6.0.1",
    "patch-package": "^6.4.7",
    "penv.macro": "^0.4.0",
    "prettier": "^2.1.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.43.3",
    "stylelint": "^13.7.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.10",
    "vite-plugin-babel-import": "^2.0.5",
    "vite-plugin-babel-macros": "^1.0.5",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-optimize-persist": "^0.1.0",
    "vite-plugin-package-config": "^0.0.3"
  },
  "dependencies": {
    "@react-spring/web": "^9.3.0",
    "antd-mobile-icons": "^0.2.2",
    "antd-mobile-v5": "npm:antd-mobile@next",
    "axios": "^0.23.0",
    "classnames": "^2.2.6",
    "localforage": "^1.7.3",
    "preact": "^10.5.15",
    "react-router-cache-route": "1.11.1",
    "react-router-dom": "^5.2.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "tslib": "^2.3.1"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": "eslint",
    "**/*.scss": "stylelint"
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "@react-spring/web",
        "antd-mobile-icons",
        "antd-mobile-v5",
        "antd-mobile-v5/es/components/action-sheet",
        "antd-mobile-v5/es/components/button",
        "antd-mobile-v5/es/components/dialog",
        "antd-mobile-v5/es/components/list",
        "antd-mobile-v5/es/components/mask",
        "antd-mobile-v5/es/components/nav-bar",
        "antd-mobile-v5/es/components/popover",
        "antd-mobile-v5/es/components/pull-to-refresh",
        "antd-mobile-v5/es/components/search-bar",
        "antd-mobile-v5/es/components/slider",
        "antd-mobile-v5/es/components/swipe-action",
        "antd-mobile-v5/es/components/toast",
        "antd-mobile-v5/es/global",
        "axios",
        "classnames",
        "localforage",
        "penv.macro",
        "preact",
        "preact/compat",
        "preact/compat/jsx-runtime",
        "preact/debug",
        "preact/devtools",
        "react-router-cache-route",
        "react-router-dom",
        "react-virtualized-auto-sizer",
        "react-window"
      ]
    }
  }
}
