        "antd-mobile/es/components/slider",
        "antd-mobile/es/components/swipe-action",
        "antd-mobile/es/components/toast",
        "antd-mobile/es/global",
        "axios",
        "classnames",
        "localforage",
        "penv.macro",
        "react",
        "react-dom",
        "react-router-dom",
        "react-virtualized-auto-sizer",
        "react-window"
      ]
    }
  }
